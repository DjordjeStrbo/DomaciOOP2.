public class Pedijatar extends Doktor {
    public Pedijatar(String ime, int godineIskustva, int brojPacijenata, String specijalizacija, int godinaDiplomiranja) {
        super(ime, godineIskustva, brojPacijenata, specijalizacija, godinaDiplomiranja);
    }

    @Override
    public abstract String leci();
       
    }
}
